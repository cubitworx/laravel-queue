{
	"name": "cubitworx/laravel-queue-test-app",
	"description": "App to test package",
	"keywords": ["test-app"],
	"license": "MIT",
	"type": "application",
	"authors": [
		{
			"name": "Charles Louw",
			"email": "charles.louw@gmail.com"
		}
	],
	"require": {
		"php": "^7.1.3",
		"fideloper/proxy": "^4.0",
		"laravel/framework": "5.8.*",
		"laravel/tinker": "^1.0"
	},
	"require-dev": {
		"filp/whoops": "^2.0",
		"fzaninotto/faker": "^1.4",
		"mockery/mockery": "^1.0",
		"nunomaduro/collision": "^2.0",
		"phpunit/phpunit": "^7.0"
	},
	"autoload": {
		"classmap": [
			"database/seeds",
			"database/factories"
		],
		"psr-4": {
			"App\\": "app/",
			"Cubitworx\\Laravel\\Queue\\": "../src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"extra": {
		"laravel": {
			"dont-discover": [
			]
		}
	},
	"scripts": {
		"post-root-package-install": [
			"@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
		],
		"post-create-project-cmd": [
			"@php artisan key:generate"
		],
		"post-autoload-dump": [
			"Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
			"@php artisan package:discover"
		]
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true,
		"optimize-autoloader": true
	},
	"minimum-stability": "dev",
	"prefer-stable": true
}
